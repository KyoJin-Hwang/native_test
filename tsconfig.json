{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "typeRoots": ["./src/types/*"],
    "types": [],
    "paths": {
      "@/*": ["./*"],
      "@/assets/*": ["assets/*", "assets"],
      "@/components/*": ["components/*", "components"],
      "@/containers/*": ["containers/*", "containers"],
      "@/views/*": ["views/*", "views"],
      "@/types/*": ["types/*", "types"]
    }
  }
}
